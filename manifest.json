{
  "manifest_version": 3,
  "name": "AI Phishing Detector",
  "version": "1.0",
  "description": "Real-time phishing website detection",
  "permissions": ["tabs", "activeTab", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
